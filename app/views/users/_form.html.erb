<%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :Surname %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :oid %>
  <%= f.email_field :oid, :value => session[:jwttokenoid], :readonly => true, class: 'form-control' %>

  <%= f.label :email %>
  <%= f.email_field :email, :value => session[:jwttokenemail], :readonly => true, class: 'form-control' %>

   <%= f.label :mobile %>
  <%= f.email_field :mobile, :value => session[:jwttokenmobile], :readonly => true, class: 'form-control' %>

  <%= f.label :membership_number %>
  <%= f.text_field :member, class: 'form-control' %>

  <%= f.label :date_of_birth %>
  <%= f.date_select :dob, class: 'form-control' %>
 
  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>